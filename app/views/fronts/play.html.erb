
<style>
html{
  height:100%
}
body{
  margin:0;
  padding:0;
  background-color:#000;
  overflow:hidden;
  height:100%
}
#game{
  position:absolute;
  top:0;
  left:0;
  width:0;
  height:0;
  overflow:hidden;
  max-width:100%;
  max-height:100%;
  min-width:100%;
  min-height:100%;
  box-sizing:border-box;
  visibility:hidden
}


body {
  position: inherit;
}

</style>
<%= render 'consent'%>
            
<iframe src="/games/<%=@game.game_id%>/" width="100%" height="100%" onload="this.width=window.innerWidth;this.height=window.innerHeight;" style="display: none;"></iframe>


